{
  "name": "minionjs-backend-mongoose",
  "description": "A Mongoose backend for minion.js",
  "version": "0.1.0",
  "main": "lib/mongoose-backend.js",
  "scripts": {
    "build": "npx tsc --build ./",
    "build:clean": "npm run clean && npm run build",
    "build:coverage": "npm run build && npm run coverage",
    "build:lint": "npm run build && npm run lint",
    "build:node": "npm run build && node",
    "build:test": "npm run build && npm test",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo lib",
    "lint": "eslint \"test/*.js\" \"src/**/*.ts\" \"src/*.ts\" \"examples/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "test": "tap -j1 --allow-incomplete-coverage test/*.js",
    "test:ci": "npm run prepublishOnly && npm run test",
    "test:debug": "npm run test -- --no-timeout"
  },
  "keywords": [
    "mongoose",
    "backend",
    "job-queue",
    "typescript",
    "node",
    "nodejs"
  ],
  "author": "Emiliano Bruni  <info@ebruni.it>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilianoBruni/minionjs-backend-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/EmilianoBruni/minionjs-backend-mongoose/issues"
  },
  "type": "module",
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/EmilianoBruni/minionjs-backend-mongoose#readme",
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/stack-utils": "^2.0.3",
    "@types/tap": "^15.0.11",
    "@types/tough-cookie": "^4.0.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tap": "^18.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@minionjs/core": "^0.3.13",
    "@mojojs/core": "^1.26.4",
    "dayjs": "^1.11.10",
    "mongoose": "^8.2.0"
  }
}
